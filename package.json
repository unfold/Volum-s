{
  "name": "Volumos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:staged": "staged-files '**/*.js' -- $(npm bin)/eslint",
    "start": "node node_modules/react-native/local-cli/cli.js start"
  },
  "pre-commit": {
    "run": "lint:staged",
    "silent": true
  },
  "eslintConfig": {
    "extends": "@unfold/eslint-config"
  },
  "dependencies": {
    "lodash": "^4.16.1",
    "qs": "^6.2.1",
    "react": "^15.4.1",
    "react-native": "^0.41.0",
    "react-native-zeroconf": "^0.6.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "styled-components": "^1.4.2",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@unfold/eslint-config": "^1.3.2",
    "eslint": "^3.10.2",
    "pre-commit": "^1.1.3",
    "react-native-git-upgrade": "^0.2.4",
    "remote-redux-devtools": "^0.5.7",
    "staged-files": "^0.1.2"
  }
}
